/system          ext4    /dev/block/platform/bootdevice/by-name/system
/system_image    emmc    /dev/block/platform/bootdevice/by-name/system
/vendor          ext4    /dev/block/platform/bootdevice/by-name/vendor
/vendor_image    emmc    /dev/block/platform/bootdevice/by-name/vendor

/boot            emmc    /dev/block/platform/bootdevice/by-name/boot
/recovery        emmc    /dev/block/platform/bootdevice/by-name/recovery
/data            ext4    /dev/block/platform/bootdevice/by-name/userdata     flags=forcefdeorfbe=/dev/block/platform/bootdevice/by-name/metadata
/cache           ext4    /dev/block/platform/bootdevice/by-name/cache
/persist         ext4    /dev/block/platform/bootdevice/by-name/persist
/misc            emmc    /dev/block/platform/bootdevice/by-name/para
/dtbo            emmc    /dev/block/platform/bootdevice/by-name/dtbo
/vbmeta          emmc    /dev/block/platform/bootdevice/by-name/vbmeta       flags=flashimg
/lk              emmc    /dev/block/platform/bootdevice/by-name/lk           flags=flashimg

/protect_f       emmc    /dev/block/platform/bootdevice/by-name/protect1     flags=backup
/protect_s       emmc    /dev/block/platform/bootdevice/by-name/protect2     flags=backup
/nvdata          emmc    /dev/block/platform/bootdevice/by-name/nvdata       flags=backup
/nvcfg           emmc    /dev/block/platform/bootdevice/by-name/nvcfg        flags=backup
/nvram           emmc    /dev/block/platform/bootdevice/by-name/nvram        flags=backup
/proinfo         emmc    /dev/block/platform/bootdevice/by-name/proinfo      flags=backup

/external_sd     vfat    /dev/block/mmcblk1p1        /dev/block/mmcblk1
/usb-otg         vfat    /dev/block/sda1             /dev/block/sda          flags=storage;wipeingui;removable
